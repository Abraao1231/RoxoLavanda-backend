generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  userName   String
  email      String
  password   String
  created_at DateTime

  @@unique([id])
  @@unique([email])
  @@map("user")
}
